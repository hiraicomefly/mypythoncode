def kangaroo(x1, v1, x2, v2):
    # Write your code here 
    if x1 == x2 and v2 == v1:
        return 'YES'
    elif x1 != x2 and v2 == v1: 
        return 'NO'
    elif x1 > x2 and v2 > v1: 
        return 'NO'
    elif x1 > x2 and v2 < v1:
        while i <= 10000:
            for i in range(1,i+1):
                if (x1 + (v1 * i)) == (x2 + (v2 * i)):
                    return 'YES'
                else :
                    return 'NO'

print(kangaroo(1571,4240,9023,4234))